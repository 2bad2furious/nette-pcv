session:
    autoStart: yes
    cookieParth: '/'
    expiration: 10 days
    identity: UserIdentity

services:
    #cacheStorage:
        #factory: TransactioningFileStorage(%tempDir%/cache)
    routerFactory: Router(%consoleMode%)
    router: @routerFactory::createRouter
    authenticator: UserManager
    authorizator: UserAuthorizator
    - ServiceLoader
    - PageManager
    - FormFactory
    - LanguageManager
    - SettingsManager
    - TagManager
    - HeaderManager
    - MediaManager

#application:
 #   mapping:
  #      admin: admin\presenter\*Presenter
   #     front: front\presenter\*Presenter

extensions:
    translation: Kdyby\Translation\DI\TranslationExtension

translation:
    debugger: on
    default: en_US

latte:
    macros:
        - Nittro\Bridges\NittroLatte\NittroMacros
